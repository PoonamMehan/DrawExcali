{
  "name": "ExcalidrawFinal",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "db:generate": "cd packages/db && npx prisma generate && cd ../..",
    "build:packages": "npm --prefix packages/db run build && npm --prefix packages/types run build"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "prisma": "^6.9.0",
    "turbo": "^2.5.4",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "apps/ws-backend",
    "packages/*"
  ],
  "dependencies": {
    "@prisma/client": "^6.9.0"
  }
}
